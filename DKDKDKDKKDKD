#include <stdio.h>
	int d[100][100];
int main( )     {
	int p, q, r;
	int a, b, c;
	printf("행수와 열수, 자료의 수를 입력하세요.\n");
	scanf("%d %d %d", &p, &q, &r);

		printf("행열값을 순서대로 입력하세요.\n");
	for(int i=0;i<r;i++){
		scanf("%d %d %d", &a, &b, &c);
		d[a-1][b-1] = c;
	}
		printf("입력 결과입니다.\n");
	for(int i=0;i<p;i++){
		for(int j=0;j<q;j++)
		printf("%d ", d[i][j]);
	        printf("\n");
	}
        printf("\n");
    } 
